{
  "name": "ismael-ramos",
  "version": "1.0.0",
  "private": true,
  "description": "Ismael Ramos resume webpage",
  "scripts": {
    "start": "ng serve --open",
    "start:ssr": "node dist/server",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "ng test",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:prod:es": "ng build --configuration=production-es",
    "build:server:prod": "ng run ismaelramos:server:production",
    "build:client-and-server-bundles": "npm run build:prod && npm run build:prod:es && npm run build:server:prod",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "ci": "npm run lint && npm run test && npm run e2e && npm run build:ssr",
    "deploy": "bash scripts/deploy.sh"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "7.0.0",
    "@angular/common": "7.0.0",
    "@angular/compiler": "7.0.0",
    "@angular/core": "7.0.0",
    "@angular/forms": "7.0.0",
    "@angular/http": "7.0.0",
    "@angular/platform-browser": "7.0.0",
    "@angular/platform-browser-dynamic": "7.0.0",
    "@angular/platform-server": "7.0.0",
    "@angular/router": "7.0.0",
    "@angular/upgrade": "7.0.0",
    "@nguniversal/express-engine": "7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "core-js": "2.5.4",
    "express": "4.16.4",
    "rxjs": "6.3.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.4",
    "@angular/cli": "7.0.4",
    "@angular/compiler-cli": "7.0.0",
    "@angular/platform-server": "7.0.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.4",
    "@types/node": "8.9.4",
    "codelyzer": "^4.5.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "phantomjs-prebuilt": "2.1.16",
    "protractor": "5.4.0",
    "ts-loader": "5.2.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.1.1",
    "webpack-cli": "3.1.0"
  },
  "repository": {}
}
